 `timescale 1ns/1ns
module TB3();
	reg [15:0] A = 0, B =0;
	reg Cin = 0;
	wire [15:0]S;
	wire Cout;

	ripple16 rip16(A, B, Cin, S, Cout);
	initial begin
	#50 A = 4'b0101000010111001;
	#50 B = 4'b1001000111100101;
	#50 Cin = 1;
	#50 A = 4'b1111000001001110;
	#50 B = 4'b1011110010101001;
	#50 Cin = 0;
	#50 A = 4'b1010010001110111;
	#50 B = 4'b0001110101100101;
	#50 A = 4'b1111000001001110;
	#50 B = 4'b1011110010101001;
	#50 Cin = 1;
	#50 A = 4'b1010010001010111;
	#50 B = 4'b0111010110100101;
	end
endmodule
